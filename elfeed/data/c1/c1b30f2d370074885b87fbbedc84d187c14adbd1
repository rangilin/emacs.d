   UnhideWhenUsed="false" Name="Dark List Accent 5"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 5"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 5"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 5"/>  <w:LsdException Locked="false" Priority="60" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Shading Accent 6"/>  <w:LsdException Locked="false" Priority="61" SemiHidden="false"    UnhideWhenUsed="false" Name="Light List Accent 6"/>  <w:LsdException Locked="false" Priority="62" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Grid Accent 6"/>  <w:LsdException Locked="false" Priority="63" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 1 Accent 6"/>  <w:LsdException Locked="false" Priority="64" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 2 Accent 6"/>  <w:LsdException Locked="false" Priority="65" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 1 Accent 6"/>  <w:LsdException Locked="false" Priority="66" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 2 Accent 6"/>  <w:LsdException Locked="false" Priority="67" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 1 Accent 6"/>  <w:LsdException Locked="false" Priority="68" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 2 Accent 6"/>  <w:LsdException Locked="false" Priority="69" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 3 Accent 6"/>  <w:LsdException Locked="false" Priority="70" SemiHidden="false"    UnhideWhenUsed="false" Name="Dark List Accent 6"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 6"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 6"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 6"/>  <w:LsdException Locked="false" Priority="19" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Emphasis"/>  <w:LsdException Locked="false" Priority="21" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Emphasis"/>  <w:LsdException Locked="false" Priority="31" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Reference"/>  <w:LsdException Locked="false" Priority="32" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Reference"/>  <w:LsdException Locked="false" Priority="33" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Book Title"/>  <w:LsdException Locked="false" Priority="37" Name="Bibliography"/>  <w:LsdException Locked="false" Priority="39" QFormat="true" Name="TOC Heading"/> </w:LatentStyles></xml><![endif]--> <!--[if gte mso 10]><style> /* Style Definitions */ table.MsoNormalTable  {mso-style-name:"Table Normal";  mso-tstyle-rowband-size:0;  mso-tstyle-colband-size:0;  mso-style-noshow:yes;  mso-style-priority:99;  mso-style-parent:"";  mso-padding-alt:0cm 5.4pt 0cm 5.4pt;  mso-para-margin:0cm;  mso-para-margin-bottom:.0001pt;  mso-pagination:widow-orphan;  font-size:12.0pt;  font-family:Cambria;  mso-ascii-font-family:Cambria;  mso-ascii-theme-font:minor-latin;  mso-hansi-font-family:Cambria;  mso-hansi-theme-font:minor-latin;} </style><![endif]-->   <!--StartFragment-->       <!--EndFragment--></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">來說說美國就業市場的激烈程度。美國地大人多，好學校多，人才多，公司的選擇很多。一個投資銀行的辦公室就那麼幾個人，哈佛同學都搶破頭了，為什麼你覺得你一去美國就可以？你覺得你是台大很棒，但是這邊有哈佛、普林斯頓哥倫比亞、有北大清華、有劍橋牛津</span>…<o:p></o:p></span></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">一個總部在</span>LA<span lang="ZH-TW">的銀行，光是招實習生就有五千人報名。這些學生都是來自名校（常春藤系列，以及地區重點大學），非名校的電腦系統會自動刪除你的履歷。你還覺得你拿著台灣的履歷線上申請紐約高盛就會被錄取嗎？</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><b><span lang="ZH-TW">身為一個外國人，為什麼公司要你？ 你的英文又不好，你的專業知識跟你同學差不多，你也不是長得驚為天人，你爸爸又不是習近平，我為什麼要你？你幫老闆寫一封郵件要花比別人長的時間，你在會議中講話可能支支吾吾語無倫次，筆記寫的又慢，講話又不好笑，別人的笑話又聽不懂</span>…<span lang="ZH-TW">我為什麼要你？</span></b><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">所以你會有很多很多</span>rejection<span lang="ZH-TW">。很多不公平、很多想哭的時候。因為這個環境就是很難很難。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="http://3.bp.blogspot.com/-NU-uGiPNpF8/Uxw7_ejENyI/AAAAAAAABMQ/1rkgJEXu2AU/s1600/2235784122_6748947057_5b848918d1_z_answer_4_xlarge.jpeg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://3.bp.blogspot.com/-NU-uGiPNpF8/Uxw7_ejENyI/AAAAAAAABMQ/1rkgJEXu2AU/s1600/2235784122_6748947057_5b848918d1_z_answer_4_xlarge.jpeg" height="214" width="320" /></a></div><div class="MsoNormal"><o:p><span style="font-family: Arial, Helvetica, sans-serif;"><br /></span></o:p></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">為了要在美國找工作，我零零總總的努力，加起來有兩年。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">曾經有半年的時間，我每天寫</span>networking email<span lang="ZH-TW">，瘋狂的去徵才活動，<b>但是半年過去了，我只拿到一個面試，而我在第一輪就搞砸了。</b></span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">後半年我回到台灣繼續做實習：邊實習的時候我繼續跟美國公司聯繫，常常不是熬夜到兩三點講越洋電話，就是六點起床</span>Skype call<span lang="ZH-TW">。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">然後我回到了美國就讀現在的研究所，每天繼續寫</span>email<span lang="ZH-TW">，準備面試。從知道我考上的那一刻起（大約是去年三月），我就開始規劃自己要找哪些公司申請，聯繫哪些學長姊；八月到了學校，我參加了每一場公司企業日，所有的職涯講座，跟學校裡最恐怖的</span>career counselor <span lang="ZH-TW">做模擬面試，厚臉皮的找不熟的美國同學組讀書會，還求好多台灣的優秀學長姊陪我練</span>case interview<span lang="ZH-TW">。功課多的時候，我會留在學校準備面試跟考試，直到半夜。回到家再繼續練習，直到早上四點。睡了四個小時以後，八點再去上課。這樣不是人過的生活，大約持續了一個學期。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">但是我還是拒絕信接不完。曾經有幾乎整整兩個月的時間，我連續每個禮拜都有兩到三個面試。但是只有少數幾個進到了</span>final round<span lang="ZH-TW">；而進到了</span>final round<span lang="ZH-TW">的公司中，沒有一個要給我</span>offer<span lang="ZH-TW">。<b><span style="color: #660000;">我每天每天都接到拒絕信，每天每天都有人否定我。</span></b>（想像每天一大清早看信箱就會看到拒絕信的感覺！）</span><o:p></o:p></span></div><div class="separator" style="clear: both; text-align: center;"><a href="http://3.bp.blogspot.com/-5aA46-9ap5k/Uxw8Tz_bmoI/AAAAAAAABMY/CHvDqufaydk/s1600/rejected.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://3.bp.blogspot.com/-5aA46-9ap5k/Uxw8Tz_bmoI/AAAAAAAABMY/CHvDqufaydk/s1600/rejected.jpg" height="133" width="200" /></a></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">記得某一次投資銀行的</span>final round<span lang="ZH-TW">的面試，我真的已經把能寫的稿子都寫了，能打的電話都打了，面試的</span>guide<span lang="ZH-TW">都念的滾瓜爛熟了。這些東西我加起來已經準備了一年。我已經模擬面試無數次，我的英文已經嚇嚇叫。擠進最後一輪了，我的現場筆試考了一百分，但是我跟面試官就是沒有那個『一拍即合』的感覺 。我不管怎麼表現，就是跟面試官很有距離感，他們就是對我冷冷淡淡。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">面試完走出辦公室，我知道這麼多的聯繫、準備，就這樣沒了。 我坐在離公司幾百公尺外的咖啡廳哭，穿著我的西裝，妝都哭花了。</span><o:p></o:p></span></div><div class="separator" style="clear: both; text-align: center;"><a href="http://2.bp.blogspot.com/-dU72W3kSuVI/Uxw8sX5qR2I/AAAAAAAABMg/QxPBnrTwHf4/s1600/cute-sad-baby.jpeg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://2.bp.blogspot.com/-dU72W3kSuVI/Uxw8sX5qR2I/AAAAAAAABMg/QxPBnrTwHf4/s1600/cute-sad-baby.jpeg" height="212" width="320" /></a></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW"><br /></span></span></div><div class="MsoNormal"><b><span style="color: #660000;"><span lang="ZH-TW" style="font-family: Arial, Helvetica, sans-serif;">但是我不能哭太久，因為我還得坐火車回家，還得繼續寄更多</span><span style="font-family: Arial, Helvetica, sans-serif;">email</span><span lang="ZH-TW" style="font-family: Arial, Helvetica, sans-serif;">，還得繼續準備下一個可能的面試。</span></span></b></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">還有一次，我為了衝刺</span>Bain<span lang="ZH-TW">的</span>final round<span lang="ZH-TW">，一個禮拜每天都忘記吃飯，就靠著三明治跟</span>energy drink <span lang="ZH-TW">裹腹。結果面試的前一天，開始上吐下洩送急診。最後結果也是沒有拿到</span>offer<span lang="ZH-TW">。（事實證明，為了短期衝刺這樣搞壞身體真的超級得不償失啊！！！）</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><b><span lang="ZH-TW">我一邊面試、一邊被拒絕、一邊又繼續準備面試</span>…</b><span lang="ZH-TW"><b>這個過程中我一點肯定都沒有得到，每天都質疑自己。</b>好多公司我花了幾個月的時間聯繫了好多學長姊，最後連一個面試都沒有；好多公司給我第一輪面試，我費寢忘時的準備，最後他們說不雇用國際學生；剩下幾家公司進入了</span>final round<span lang="ZH-TW">，就是沒有上。<b><span style="color: #660000;">兩年的時間，我的結果是零。</span></b></span><o:p></o:p></span></div><div class="separator" style="clear: both; text-align: center;"><a href="http://2.bp.blogspot.com/-PUsCSiowY6E/Uxw9kJ1qx-I/AAAAAAAABMs/Jh93pecnW1c/s1600/tooopen_201210211224113641.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://2.bp.blogspot.com/-PUsCSiowY6E/Uxw9kJ1qx-I/AAAAAAAABMs/Jh93pecnW1c/s1600/tooopen_201210211224113641.jpg" height="240" width="320" /></a></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">某一天我覺得我太累了，已經不想撐下去了，毅然決然訂了寒假回台灣的機票，暗自決定：把最後這兩個禮拜的面試衝刺完，我就休息，寒假時繼續準備春季的求職潮吧！</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">於是在最後的兩個禮拜我繼續燃燒（我都變成面試機器啦！），這時的英文已經到了可以見人說人話，見鬼說鬼話的地步了。我的最後一個面試，是一家很夢幻的</span>economic consulting firm<span lang="ZH-TW">，薪水高，又給自己的辦公室，做過好多有名的案子，令人垂涎三尺。領我進去一關一關面試的是個賓州大學畢業的學長。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">當時的我幾乎已經處在神智不清的模式（因為睡太少），面試的狀況不怎麼佳，但是還是把該講的都講了，該表現的都表現了，<b>用盡我的最後一絲力氣燃燒，想要為我這階段的求職做個小句點，然後，下學期再出發。</b></span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">一個禮拜過後，當我正在圖書館拯救快要被當的會計期中考時，一通電話打來，是那家公司。他們很有禮貌的告訴我，他們非常高興在這個過程中認識我</span>…<span lang="ZH-TW">當下我馬上知道這是一通友善的拒絕電話。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">『</span>…<span lang="ZH-TW">所以我們決定要給你一個</span>offer<span lang="ZH-TW">。』</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">然後我不敢相信自己的耳朵。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">接下來的一切都很模糊，電話那頭說的</span>offer<span lang="ZH-TW">內容我幾乎沒有在聽了；只知道內心一直尖叫，想要馬上打給爸爸媽媽。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">我就這樣傻呼呼的在圖書館外喜極而泣，邊哭邊跳舞。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW"><b>我萬萬沒有想到，就在兩年以後，充滿了拒絕的兩年以後，終於有公司要我了。</b>幾百封的</span>email<span lang="ZH-TW">、幾百通電話、幾十個面試</span>…<span lang="ZH-TW">整整兩年沒有公司要我，<b>但是在我要放棄的最後最後一刻</b>，我走到了。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW"><b>所以，身為國際學生的我們，面對的是很多、很多的困難。很小、很小的機會。<span style="color: #660000;">但是如果你真的真的想要，你就可以得到。</span></b></span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">不要覺得申請上就結束了、申請上就是對自己跟爸媽的交代、申請上就是終點。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">申請上以後，有的是更長遠、更激烈的競爭。但是你知道嗎，你真的走的到，我就這樣土法煉鋼一小步一小步，很厚臉皮的，很慢很慢的，走到了。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><i><span lang="ZH-TW">（至於詳細怎麼做的？我的</span>email<span lang="ZH-TW">怎麼寫，面試怎麼準備？待我整理整理以後，還會跟大家分享的。）</span></i><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">所以啊，申請上或是正在申請的你，不要覺得一切快要結束了。你眼前的路還很遠，山頭還很高。</span><o:p></o:p></span></div><div class="MsoNormal"><br /></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW">但是路上的風景，山頂的壯闊，一定是你沒有看過的美麗。</span><o:p></o:p></span></div><div class="separator" style="clear: both; text-align: center;"><a href="http://1.bp.blogspot.com/-Ac4LQjAdTww/Uxw-EyRZenI/AAAAAAAABM0/1RVnOxhvGT4/s1600/persistenceisachoice.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://1.bp.blogspot.com/-Ac4LQjAdTww/Uxw-EyRZenI/AAAAAAAABM0/1RVnOxhvGT4/s1600/persistenceisachoice.jpg" height="212" width="320" /></a></div><div class="MsoNormal"><span style="font-family: Arial, Helvetica, sans-serif;"><span lang="ZH-TW"><br /></span></span></div><!--[if gte mso 9]><xml> <o:OfficeDocumentSettings>  <o:AllowPNG/> </o:OfficeDocumentSettings></xml><![endif]--> <!--[if gte mso 9]><xml> <w:WordDocument>  <w:View>Normal</w:View>  <w:Zoom>0</w:Zoom>  <w:TrackMoves/>  <w:TrackFormatting/>  <w:PunctuationKerning/>  <w:ValidateAgainstSchemas/>  <w:SaveIfXMLInvalid>false</w:SaveIfXMLInvalid>  <w:IgnoreMixedContent>false</w:IgnoreMixedContent>  <w:AlwaysShowPlaceholderText>false</w:AlwaysShowPlaceholderText>  <w:DoNotPromoteQF/>  <w:LidThemeOther>EN-US</w:LidThemeOther>  <w:LidThemeAsian>JA</w:LidThemeAsian>  <w:LidThemeComplexScript>X-NONE</w:LidThemeComplexScript>  <w:Compatibility>   <w:BreakWrappedTables/>   <w:SnapToGridInCell/>   <w:WrapTextWithPunct/>   <w:UseAsianBreakRules/>   <w:DontGrowAutofit/>   <w:SplitPgBreakAndParaMark/>   <w:EnableOpenTypeKerning/>   <w:DontFlipMirrorIndents/>   <w:OverrideTableStyleHps/>   <w:UseFELayout/>  </w:Compatibility>  <m:mathPr>   <m:mathFont m:val="Cambria Math"/>   <m:brkBin m:val="before"/>   <m:brkBinSub m:val="&#45;-"/>   <m:smallFrac m:val="off"/>   <m:dispDef/>   <m:lMargin m:val="0"/>   <m:rMargin m:val="0"/>   <m:defJc m:val="centerGroup"/>   <m:wrapIndent m:val="1440"/>   <m:intLim m:val="subSup"/>   <m:naryLim m:val="undOvr"/>  </m:mathPr></w:WordDocument></xml><![endif]--><!--[if gte mso 9]><xml> <w:LatentStyles DefLockedState="false" DefUnhideWhenUsed="true"   DefSemiHidden="true" DefQFormat="false" DefPriority="99"   LatentStyleCount="276">  <w:LsdException Locked="false" Priority="0" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Normal"/>  <w:LsdException Locked="false" Priority="9" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="heading 1"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 2"/>  <w:LsdException Locked="fals